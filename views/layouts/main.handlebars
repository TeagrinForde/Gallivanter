<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Gallivanter</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css" />
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
  <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
  <link rel="stylesheet" href="/css/style.css" />
</head>

<body>
  <header>

    {{! logo and title display }}
    <div class="navbar-fixed">

      <nav class="cyan">
        <div class="nav-wrapper">
          <a href="/" class="brand-logo indigo-text"><img src="images/logo.png" height="30" width="30" type="image/png"
              alt="Logo" />
            Gallivanter</a>

          {{! Full Nav }}
          <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
          <ul class="right hide-on-med-and-down">
            <li>
              {{#if loggedIn}}
              <a id="logout">Logout</a>
              {{else}}
              <a href="/login">Login</a>
              {{/if}}
            </li>
            <li><a href="/newTrip">NEW Trip</a></li>
            <li><a href="/tripData">My Trips</a></li>
            <li><a href="#contact">Contact Us</a></li>
          </ul>
        </div>
      </nav>

      {{! Dropdown Nav }}
      <ul id="mobile-demo" class="sidenav">
        <li>
          <div class="user-view">
            <div class="background">
              <img src="http://www.accurateglobalreach.com/wp-content/uploads/2019/07/Travel.jpg" alt="travel">
            </div>
            <a href="#logo"><img class="circle" src="/images/logo.png" /></a>
            <a href="#gallivanter"><span class="white-text name">Gallivanter</span></a>
            <a href="#description"><span class="white-text email"></span></a>
          </div>
        </li>
        <li><a class="waves-effect" href="/login"><i class="material-icons">thumb_up</i>Login</a></li>

        <li>
          <div class="divider"></div>
        </li>
        <li><a class="waves-effect" href="/newTrip">NEW Trip</a></li>

        <li>
          <div class="divider"></div>
        </li>
        <li><a class="waves-effect" href="/tripData">My Trips</a></li>

        <li>
          <div class="divider"></div>
        </li>
        <li><a class="waves-effect" href="#contact">Contact Us</a></li>
      </ul>
      <a href="#" data-target="slide-out" class="sidenav-trigger"><i class="material-icons">menu</i></a>
    </div>
  </header>

  <main class="row">

    <div class="col s12" id="top">
      {{{body}}}
    </div>

  </main>

  <footer class="page-footer cyan indigo-text">
    <div class="container">
      <div class="row">
        <div class="col l6 s12">
          <h5><img src="images/logo.png" height="50" width="50" type="image/png" alt="Gallivantor Logo" />
            Gallivanter Travels</h5>
          <p>Not all who wander, are lost.</p>
        </div>
        <div class="col l4 offset-l2 s12">
          <h5 id="contact">Contact Us</h5>
          <ul>
            <li><a href="https://github.com/TeagrinForde">
                <div class="chip">
                  <img src="/images/Profile TF.jpg" alt="Teagrin Forde" />
                  Teagrin Forde
                </div>
              </a></li>
            <li><a href="https://github.com/vyang14">
                <div class="chip">
                  <img src="/images/Profile VY.PNG" alt="Vince Yang" />
                  Vince Yang
                </div>
              </a></li>
            <li><a href="https://github.com/fairiberry">
                <div class="chip">
                  <img src="/images/Profile FS.PNG" alt="Farrah Syed" />
                  Farrah Syed
                </div>
              </a></li>
            <li><a href="https://github.com/Groceat">
                <div class="chip">
                  <img src="/images/Profile CW.PNG" alt="Charles Wright" />
                  Charles Wright
                </div>
              </a></li>
          </ul>
        </div>
      </div>
    </div>
    <div class="footer-copyright">
      <div class="container">
        Â© 2022 Gallivanter
        <a class="right indigo-text" href="#top">Back to top</a>
      </div>
    </div>
  </footer>

  <script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAigt9WzHgkJnidN6iEN2Uq0KMSEo8lef0&callback=initMap&v=weekly"
    defer></script>
  <script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAigt9WzHgkJnidN6iEN2Uq0KMSEo8lef0&libraries=places"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script>
    $(document).ready(function () {
      $('.sidenav').sidenav();
    });
    
    $(document).ready(function () {
      $('.carousel').carousel();
    });

    $(".button-collapse").sideNav();
  </script>
</body>

</html>